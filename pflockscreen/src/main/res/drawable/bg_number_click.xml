<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- NOTE: order is important (the first matching state(s) is what is rendered) -->
    <item android:state_focused="true" android:state_pressed="false" android:drawable="@drawable/circle_code_fill_pf_lockscreen" />
    <item android:state_focused="true" android:state_pressed="true" android:drawable="@drawable/circle_code_fill_pf_lockscreen" />
    <item android:state_focused="false" android:state_pressed="true" android:drawable="@drawable/circle_code_fill_pf_lockscreen"/>
    <item android:drawable="@drawable/bg_number" />
</selector>
